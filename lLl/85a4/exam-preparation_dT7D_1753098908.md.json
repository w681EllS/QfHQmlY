{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "guard-for-in": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-underscore-dangle": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "class-methods-use-this": "off"
    }
}
